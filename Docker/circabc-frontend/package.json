{
  "name": "circabc",
  "version": "4.1.4",
  "license": "EUPL-1.1",
  "engines": {
    "node": ">=10.9.0 <13.3.0",
    "yarn": ">=1.21.1 <2"
  },
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "ng-version": "ng version",
    "ng-update": "ng update @angular/core @angular/cli --next --force  --allow-dirty",
    "ng-update-cli": "ng update @angular/cli --from 9 --to 10 --migrate-only --allow-dirty",
    "ng-update-core": "ng update @angular/core --from 9 --to 10 --migrate-only --allow-dirty",
    "ng-update-one": "ng update  @angular/core --migrate-only migration-v9-module-with-providers --allow-dirty",
    "ng-update-help": "ng update --help",
    "start": "ng serve -o",
    "build": "ng build",
    "build-prod-profile": "ng build --prod  --profile",
    "build-prod": "ng build --prod",
    "dcp-npx": "npx jscpd  src --format \"typescript,markup,scss\"  --ignore \"src/app/core/generated/**/*.ts\" --min-lines 30 --max-lines 2000 ",
    "dcp": "jscpd  src --format \"typescript,markup,scss\"  --ignore \"src/app/core/generated/**/*.ts\" --min-lines 30 --max-lines 2000 ",
    "gulp-format": "gulp  prettier",
    "gulp-format-scss": "gulp  format:scss ",
    "format-check": "prettier --single-quote --list-different --html-whitespace-sensitivity strict \"src/**/*.html\" \"src/**/*.ts\"  \"!src/app/core/generated/**/*.ts\" \"!src/app/app-info.ts\" ",
    "format-fix": "prettier --write --single-quote --list-different --html-whitespace-sensitivity strict \"src/**/*.html\" \"src/**/*.ts\"  \"!src/app/core/generated/**/*.ts\" \"!src/app/app-info.ts\" ",
    "format-fix-gen": "prettier --write --single-quote --list-different \"src/app/core/generated/**/*.ts\" ",
    "format-check-crlf": "prettier --single-quote --list-different  \"src/**/*.ts\"  \"!src/app/core/generated/**/*.ts\" \"!src/app/app-info.ts\" --end-of-line=crlf ",
    "format-check-scss": "prettier --single-quote --list-different  \"src/**/*.scss\" ",
    "prettier": "prettier --version",
    "lf-gen": "crlf --set=LF \"src/app/core/generated/**/*.ts",
    "lf-all": "crlf --set=LF \"src/**/*.ts",
    "image-compress": "gulp imagemin",
    "unittest": "ng test",
    "unit-tests": "ng test --sourcemap=true",
    "test-headless": "ng test --browser ChromeHeadless --single-run",
    "tsc-version": "tsc  --version",
    "tsc": "tsc  --project src/tsconfig.app.json --pretty",
    "tsc-watch": "tsc  --project src/tsconfig.app.json --pretty --watch",
    "tsc-heap": "node --max-old-space-size=8192 ./node_modules/typescript/lib/tsc.js  --project src/tsconfig.app.json --pretty",
    "stylelint": "node ./node_modules/stylelint/bin/stylelint.js \"src/**/*.scss\" --syntax scss",
    "stylelint-html": "node ./node_modules/stylelint/bin/stylelint.js \"src/**/*.html\" ",
    "stylelint-fix": "node ./node_modules/stylelint/bin/stylelint.js \"src/**/*.scss\" --syntax scss --fix",
    "stylelint-html-fix": "node ./node_modules/stylelint/bin/stylelint.js \"src/**/*.html\" --fix ",
    "lint": "ng lint circabc --format msbuild",
    "lint-force": "ng lint circabc --force  --format msbuild",
    "lint-fix": "ng lint circabc --fix --force  --format msbuild",
    "linte2e": "ng lint circabc-e2e --force  --format msbuild",
    "linte2e-fix": "ng lint circabc-e2e --fix --force  --format msbuild",
    "e2e": "ng e2e",
    "e2e-tomcat": "ng e2e --project=circabc-e2e --dev-server-target=",
    "e2e-compile": "tsc  --project \"e2e/tsconfig.e2e.json\" --outDir \"dist/out-tsc-e2e\" --pretty",
    "build-stat": "ng build --prod --stats-json",
    "build-stat-vsm": "ng build --prod --source-map --vendor-source-map",
    "build-stat-heap": "npm run build-stat  --max-old-space-size=4096",
    "bundle-report": "webpack-bundle-analyzer dist/circabc/stats.json",
    "code-coverage": "ng test --watch=false --code-coverage",
    "doc-build": "./node_modules/.bin/compodoc -p tsconfig.json",
    "doc-serve": "http-serve ./documentation -p 9090",
    "doc-build-serve": "./node_modules/.bin/compodoc -p tsconfig.json -s -r 9090 -o",
    "get-build-date": "node getBuildDate.js",
    "http-serve": "http-serve ./dist/circabc -p 4200",
    "http-serve-gzip": "http-serve ./dist/circabc -p 4200 --gzip ",
    "bamboo": "npm run get-build-date && npm run tsc && npm run format-check && npm run lint && npm run stylelint && npm run build-prod ",
    "bamboo-crlf": "npm run get-build-date && npm run tsc && npm run format-check-crlf && npm run lint && npm run stylelint && npm run build-prod ",
    "build-circabc1213-atelier-dev1": "npm run get-build-date && ng build -c=circabc1213-atelier-dev1",
    "build-training": "npm run get-build-date && ng build -c=training",
    "localhost": "npm run get-build-date && ng build --prod  -c=localhost",
    "docker": "npm run get-build-date && ng build --prod  -c=docker && gzipper compress --verbose ./dist/circabc",
    "docker-it-nrw": "npm run get-build-date && ng build --prod  -c=docker-it-nrw && gzipper compress --verbose ./dist/circabc",
    "build-tomcat": "npm run get-build-date && ng build -c=tomcat",
    "tomcat": "npm run get-build-date && ng serve -c=tomcat -o",
    "test": "npm run get-build-date && ng build --prod  -c=test",
    "stress": "npm run get-build-date && ng build -c=stress",
    "development": "npm run get-build-date && ng build --prod -c=development",
    "testenv": "npm run get-build-date && ng build --prod  -c=test",
    "acceptance": "npm run get-build-date && ng build --prod  -c=acceptance",
    "production": "npm run get-build-date && ng build --prod  -c=production",
    "build-scircabc-acc": "npm run get-build-date && ng build --prod  -c=scircabc-acc",
    "build-scircabc-prod": "npm run get-build-date && ng build --prod  -c=scircabc-prod",
    "build-shs-acceptance": "npm run get-build-date && ng build --prod  -c=shs-acceptance",
    "build-shs-prod": "npm run get-build-date && ng build --prod  -c=shs-prod",
    "build-enisa-acceptance": "npm run get-build-date && ng build --prod  -c=enisa-acceptance",
    "build-enisa-prod": "npm run get-build-date && ng build --prod  -c=enisa-prod",
    "ncu": "npx npm-check-updates",
    "ncu-u": "npx npm-check-updates -u",
    "browserslist": "npx browserslist",
    "check": "npx npm-check -u",
    "link-status": "npx link-status -p",
    "npx-http-serve": "npx http-serve ./dist/circabc -p 4200",
    "gen-version": "openapi-generator version",
    "circabc-api": "rimraf src/app/core/generated/circabc && openapi-generator generate -i apis/openapi.yaml -g typescript-angular -o src/app/core/generated/circabc --additional-properties ngVersion=9.0.0 -D providedInRoot=true && npm run format-fix-gen",
    "ares-bridge-api": "rimraf src/app/core/generated/ares-bridge && openapi-generator generate -i apis/ares-bridge.yaml -g typescript-angular -o src/app/core/generated/ares-bridge --additional-properties ngVersion=9.0.0 -D providedInRoot=true && npm run format-fix-gen",
    "tomcat-prod-it-nrw": "npm run get-build-date && ng build --prod  -c=tomcat-prod-it-nrw && gzipper compress --verbose ./dist/circabc"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "10.1.2",
    "@angular/animations": "10.1.2",
    "@angular/cdk": "10.2.1",
    "@angular/common": "10.1.2",
    "@angular/compiler": "10.1.2",
    "@angular/core": "10.1.2",
    "@angular/forms": "10.1.2",
    "@angular/platform-browser": "10.1.2",
    "@angular/platform-browser-dynamic": "10.1.2",
    "@angular/router": "10.1.2",
    "@angular/service-worker": "10.1.2",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "core-js": "2.6.9",
    "file-saver": "2.0.2",
    "font-awesome": "4.7.0",
    "ngx-clipboard": "13.0.1",
    "ngx-cookie-service": "10.0.1",
    "ngx-extended-pdf-viewer": "4.1.2",
    "primeicons": "4.0.0",
    "primeng": "10.0.0",
    "quill": "1.3.7",
    "rxjs": "6.6.3",
    "ts-helpers": "1.1.2",
    "ubuntu-fontface": "0.1.13",
    "zone.js": "0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1001.2",
    "@angular-devkit/build-optimizer": "0.1001.2",
    "@angular/cli": "10.1.2",
    "@angular/compiler-cli": "10.1.2",
    "@compodoc/compodoc": "1.1.11",
    "@openapitools/openapi-generator-cli": "1.0.18-4.3.1",
    "@types/file-saver": "2.0.1",
    "@types/jasmine": "3.5.14",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.10.3",
    "clang-format": "1.4.0",
    "codelyzer": "6.0.0",
    "crlf": "1.1.1",
    "gulp": "4.0.2",
    "gulp-clang-format": "1.0.27",
    "gulp-imagemin": "7.1.0",
    "gulp-prettier": "3.0.0",
    "gzipper": "4.0.2",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.2",
    "jscpd": "3.3.19",
    "karma": "5.2.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "prettier": "2.1.2",
    "protractor": "7.0.0",
    "rimraf": "3.0.2",
    "stylelint": "13.7.1",
    "stylelint-config-standard": "20.0.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "6.2.0",
    "tslint-no-circular-imports": "0.7.0",
    "typescript": "4.0.2",
    "webpack-bundle-analyzer": "3.8.0"
  }
}
